{
	"name": "Fit_Fitness_API",
	"version": "1.0.0",
	"main": "app.js",
	"license": "MIT",
	"scripts": {
		"start": "yarn && node ./bin/www",
		"debug": "nodemon --inspect-brk=9229 app.js",
		"build": "docker build -t api . && docker run -p 3000:3000 api",
		"containers": "docker ps -a",
		"stop": "docker stop api_container",
		"rm_containers": "docker stop $(docker ps -aq) && docker rm $(docker ps -aq)",
		"stop_running": "docker stop $(docker ps -aq)"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^16.3.1",
		"express": "~4.16.1",
		"helmet": "^4.2.0",
		"jsonwebtoken": "^8.5.1",
		"knex": "^2.4.2",
		"morgan": "~1.9.1",
		"nodemon": "^2.0.22",
		"pg": "^8.4.2",
		"uuid": "^9.0.0"
	}
}
